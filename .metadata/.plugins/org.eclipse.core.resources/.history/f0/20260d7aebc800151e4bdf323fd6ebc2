import java.util.ArrayList;

public class Main {
	
	private static ArrayList <Bike> allbikes; 
	
	public static void main(String[] args) {
		
		for (int i = 0; i < 10; i++){
			allbikes.add(new Bike ("Red", 100, 12));
			System.out.println("Bike " + String.valueOf(i) + " has the price " + Bike.getPrice() + " and the size " + Bike.getSize());
		}

		
	
		
	}

	
	
	
}
